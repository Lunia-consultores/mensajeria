  "description": "Conjunto de utilidades para mensajería asíncrona",
  "minimum-stability": "stable",
  "version": "0.1",
  "license": "Lunia consultores",
  "authors": [
    {
      "name": "Jaime Mulero",
      "email": "jmulero@lunia.es"
    }
  ],
  "require": {
    "php": "^7.3|^7.4|^8.0|^8.1",
    "php-amqplib/php-amqplib": "^3.2",
